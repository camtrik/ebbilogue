{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "start": "next dev -H 0.0.0.0 -p 6060",
    "dev": "cross-env INIT_CWD=$PWD next dev -H  0.0.0.0 -p 6060",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky",
    "compress": "node ./scripts/compress.mjs"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "latest",
    "@algolia/autocomplete-plugin-algolia-insights": "latest",
