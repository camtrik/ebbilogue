    "image-size": "1.0.0",
    "lucide-react": "^0.474.0",
    "next": "^14.2.23",
    "next-auth": "^4.24.11",
    "next-contentlayer2": "0.4.4",
    "next-themes": "^0.3.0",
    "open": "^10.1.0",
    "pliny": "0.2.0",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-activity-calendar": "^2.2.8",
    "react-dom": "18.2.0",
    "react-icons": "^5.2.0",
    "react-responsive": "^10.0.0",
    "react-responsive-masonry": "^2.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
